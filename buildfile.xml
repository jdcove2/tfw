<project name="TheFramework" default="help" basedir=".">

	<target name="help">
		<echo message="Usage for ImmutableLongArray ANT build file:" />
		<echo message="    help - show this usage text." />
	</target>
	
	<patternset id="ila_source_files">
		<include name="buildfile.xml"/>
		<include name="tfw/**/*.mapping"/>
		<include name="tfw/**/*.template"/>
		<include name="tfw/**/*.java"/>
	</patternset>
	
	<patternset id="class_files">
		<include name="tfw/**/*.class"/>
	</patternset>
	
	<fileset id="ila_source" dir="${basedir}">
		<patternset refid="ila_source_files"/>
	</fileset>
	
	<fileset id="class" dir="${basedir}">
		<patternset refid="class_files"/>
	</fileset>
	
	<target name="createIlaJars">
		<jar jarfile="${basedir}/TheFramework_src.jar">
			<fileset refid="ila_source"/>
		</jar>
		<jar jarfile="${basedir}/TheFramework.jar">
			<fileset refid="class"/>
		</jar>
	</target>

